{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Supa",
  "version": "0.1.0",
  "identifier": "cloud.supa.supa",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "Supa",
        "width": 800,
        "height": 600,
        "visible": true,
        "dragDropEnabled": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEVDMjUzODdGQjk1RDA3NTgKUldSWUIxMjVmemdsN0xnWXM3WFhabC9iYTVEMk8wTVRsZ2J1dUxldGkwRmgrUkx5WlE4Uy85U28K",
      "endpoints": [
        "https://github.com/mitkury/supa/releases/latest/download/latest.json"
      ]
    }
  }
}
